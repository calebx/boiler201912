{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "plugins": ["react"],
  "rules": {
    "no-console": "off",
    "indent": ["error", 4],
    "comma-dangle": ["error", "never"],
    "quotes": ["error", "double", { "avoidEscape": true }],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
